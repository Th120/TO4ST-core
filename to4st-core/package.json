{
  "name": "to4st-core",
  "version": "0.7.0",
  "description": "TO4ST-core",
  "author": "Th120",
  "license": "MIT",
  "scripts": {
    "migrationConfigTypeormSQLite": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm --config ./migrations-sqlite-config.json",
    "migrationConfigTypeormPostgres": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm --config ./migrations-postgres-config.json",
    "migrationConfigTypeormMySQL": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm --config ./migrations-mysql-config.json",
    "runMigrationsSQLite": "yarn migrationConfigTypeormSQLite migration:run",
    "runMigrationsPostgres": "yarn migrationConfigTypeormPostgres migration:run",
    "runMigrationsMySQL": "yarn migrationConfigTypeormMySQL migration:run",
    "generate:migrationsSQLite": "yarn migrationConfigTypeormSQLite migration:generate",
    "generate:migrationsPostgres": "yarn migrationConfigTypeormPostgres migration:generate",
    "generate:migrationsMySQL": "yarn migrationConfigTypeormMySQL migration:generate",
    "updateMigrationsSQLite": "yarn runMigrationsSQLite && yarn generate:migrationsSQLite",
    "updateMigrationsPostgres": "yarn runMigrationsPostgres && yarn generate:migrationsPostgres",
    "updateMigrationsMySQL": "yarn runMigrationsMySQL && yarn generate:migrationsMySQL",
    "prebuild": "rimraf dist",
    "generate:clientCore": "zeus http://localhost:3000/graphql ./src/libs/client/generated-core --ts",
    "generate:clientMaster": "zeus http://localhost:3001/graphql ./src/libs/client/generated-master --ts",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --runInBand",
    "test:watch": "jest --watch --runInBand",
    "test:cov": "jest --coverage --runInBand",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --detectOpenHandles --config ./test/jest-e2e.json --runInBand"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.5",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.5",
    "@nestjs/graphql": "^8.0.2",
    "@nestjs/platform-express": "^8.0.5",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/typeorm": "^8.0.1",
    "@node-steam/id": "^1.2.0",
    "apollo-server-express": "2.x.x",
    "async-await-foreach": "^1.0.4",
    "async-mutex": "^0.3.1",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "graphql": "^15.5.1",
    "graphql-tools": "^8.0.0",
    "iso8601-validator": "^1.0.1",
    "joi": "^17.4.1",
    "json-stringify-pretty-compact": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "jssha": "^3.2.0",
    "lodash": "^4.17.21",
    "lru-cache": "^6.0.0",
    "memoizee": "^0.4.15",
    "moment": "^2.27.0",
    "mysql": "^2.18.1",
    "nanoid": "^3.1.23",
    "p-retry": "^4.6.1",
    "pg": "^8.7.1",
    "public-ip": "^4.0.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.3.0",
    "sqlite3": "^5.0.2",
    "string-hash": "^1.1.3",
    "ts-morph": "^11.0.3",
    "typeorm": "0.2.35",
    "validator": "^13.6.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.0",
    "@nestjs/schematics": "^8.0.2",
    "@nestjs/testing": "^8.0.5",
    "@types/bcrypt": "^5.0.0",
    "@types/chance": "^1.1.3",
    "@types/express": "^4.17.13",
    "@types/jest": "26.0.24",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/lodash": "^4.14.171",
    "@types/lru-cache": "^5.1.1",
    "@types/memoizee": "^0.4.6",
    "@types/moment": "^2.13.0",
    "@types/nanoid": "^3.0.0",
    "@types/node": "^14.17.19",
    "@types/p-retry": "^3.0.1",
    "@types/public-ip": "^3.1.0",
    "@types/string-hash": "^1.1.1",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "chance": "^1.1.6",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "graphql-zeus": "^3.1.0",
    "jest": "^27.0.6",
    "jest-chance": "^0.1.8",
    "jest-extended": "^0.11.5",
    "prettier": "^2.3.2",
    "supertest": "^6.1.4",
    "ts-jest": "27.0.4",
    "ts-loader": "^9.2.4",
    "ts-node": "^10.1.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "ignoreCoverageForDecorators": true,
        "ignoreCoverageForAllDecorators": true
      }
    },
    "globalSetup": "jest-chance",
    "setupFilesAfterEnv": [
      "jest-extended"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.ts",
      "!**/*.{resolver,module,entity}.ts",
      "!**/node_modules/**",
      "!**/shared/**",
      "!**/libs/**",
      "!**/migrations/**",
      "!**/test/**"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "testTimeout": 60000
  }
}
